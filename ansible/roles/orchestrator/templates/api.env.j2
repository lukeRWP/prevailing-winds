# Orchestrator API environment â€” managed by Ansible
NODE_ENV=production
PORT={{ orchestrator_port }}
ORCHESTRATOR_HOME={{ orchestrator_home }}
VAULT_ADDR={{ vault_addr }}
ANSIBLE_VENV={{ orchestrator_venv }}
{% if orchestrator_admin_token is defined %}
ADMIN_TOKEN={{ orchestrator_admin_token }}
{% endif %}
{% for token in orchestrator_app_tokens | default([]) %}
APP_TOKEN_{{ token.app | upper }}={{ token.value }}
{% endfor %}
{% if vault_role_id is defined %}
VAULT_ROLE_ID={{ vault_role_id }}
{% endif %}
{% if vault_secret_id is defined %}
VAULT_SECRET_ID={{ vault_secret_id }}
{% endif %}
GIT_SSH_COMMAND=ssh -i {{ orchestrator_home }}/.ssh/deploy_key -o StrictHostKeyChecking=accept-new
